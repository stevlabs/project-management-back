{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",   
        "include": ["./"],                        
        "exclude": [
          "node_modules/",
          "docs"
        ]              
    },
    "plugins": [],
    "recurseDepth": 10,                         
    "opts": {
        "destination": "./docs/",          
        "recurse": true   
    }                       
}